services:
  onlineenterprise.web:
    build:
      context: C:\Users\anduser\source\repos\OnlineEnterpriseExmpl
      dockerfile: OnlineEnterprise.Web/Dockerfile
      target: base
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44363'
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: onlineenterpriseweb:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.2/OnlineEnterprise.Web.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 51490
      target: 80
    - published: 44363
      target: 443
    volumes:
    - C:\Users\anduser\source\repos\OnlineEnterpriseExmpl\OnlineEnterprise.Web:/app:rw
    - C:\Users\anduser\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Users\anduser\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\anduser\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\anduser\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'